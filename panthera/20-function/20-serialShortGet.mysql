CREATE OR REPLACE FUNCTION `serialShortGet`(
  INAME VARCHAR(32) CHARSET utf8
) RETURNS VARCHAR(32) MODIFIES SQL DATA
BEGIN
    RETURN ( SELECT 
      CONCAT(
        SUBSTRING(MD5(RAND()),10,1),
        CONV((SELECT serialGet(INAME)), 10, 32),
        SUBSTRING(MD5(RAND()),6,1)
      )
    );
END;
